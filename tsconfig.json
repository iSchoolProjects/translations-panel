{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6","es2019", "es2020.promise", "es2020.bigint", "es2020.string", "dom"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
   },
   "exclude": ["frontend"]
}